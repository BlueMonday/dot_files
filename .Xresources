! Molokai theme
URxvt*background: #101010
URxvt*foreground: #d0d0d0
URxvt*color0: #101010
URxvt*color1: #960050
URxvt*color2: #66aa11
URxvt*color3: #c47f2c
URxvt*color4: #30309b
URxvt*color5: #7e40a5
URxvt*color6: #3579a8
URxvt*color7: #9999aa
URxvt*color8: #303030
URxvt*color9: #ff0090
URxvt*color10: #80ff00
URxvt*color11: #ffba68
URxvt*color12: #5f5fee
URxvt*color13: #bb88dd
URxvt*color14: #4eb4fa
URxvt*color15: #d0d0d0

URxvt.perl-ext-common: default,clipboard,matcher

! matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1

! clipboard
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

URxvt.scrollBar: false
URxvt.font: xft:Dejavu\ Sans\ Mono:pixelsize=18,xft:Dejavu\ Sans\ Mono\ for\ Powerline:pixelsize=18
URxvt.boldFont: xft:Dejavu\ Sans\ Mono:pixelsize=18,xft:Dejavu\ Sans\ Mono\ for\ Powerline:pixelsize=18
URxvt.cursorBlink: true
Urxvt.loginShell: true
URxvt.transparent: true
URxvt.shading: 20

Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
Xft.rgba:       rgb
Xft.hintstyle:  hintslight
